<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="solent.ac.uk.ood.examples.hotellock.reception.Controller">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <FlowPane prefHeight="223.0" prefWidth="600.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="600.0" text="Create A New Card">
                     <font>
                        <Font size="22.0" />
                     </font>
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label prefWidth="200.0" text="Room Number">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <TextField id="ccRoomNumber" fx:id="ccRoomNumber" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Label prefWidth="200.0" text="Valid From Date">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField id="ccValidFrom" fx:id="ccValidFrom" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Label prefWidth="200.0" text="Expiration Date">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField id="ccExpirationDate" fx:id="ccExpirationDate" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Button id="createCard" fx:id="createCard" mnemonicParsing="false" onAction="#guiCreateCardCode" text="Create Card">
                     <FlowPane.margin>
                        <Insets left="410.0" right="90.0" top="10.0" />
                     </FlowPane.margin></Button>
                  <Label prefWidth="200.0" text="Card Code">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField fx:id="ccCardCode" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets top="10.0" />
                     </FlowPane.margin>
                  </TextField>
               </children>
            </FlowPane>
            <FlowPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="600.0" text="Check A Card">
                     <font>
                        <Font size="22.0" />
                     </font>
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label prefWidth="200.0" text="Card Code">
                     <FlowPane.margin>
                        <Insets left="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField id="checkCardCode" fx:id="checkCardCode" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Label prefWidth="200.0" text="Room Number">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label id="checkRoomNumber" fx:id="checkRomNumber" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <Label prefWidth="200.0" text="Valid From Date">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <Label id="checkValidFrom" fx:id="checkValidFrom" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <Label prefWidth="200.0" text="Expiration date">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <Label id="checkExpirationDate" fx:id="checkExpirationDate" prefWidth="300.0">
                     <FlowPane.margin>
                        <Insets right="90.0" top="10.0" />
                     </FlowPane.margin></Label>
                  <Button id="checkCard" fx:id="checkCard" mnemonicParsing="false" onAction="#guiCheckCardCode" text="Check Card">
                     <FlowPane.margin>
                        <Insets left="410.0" top="10.0" />
                     </FlowPane.margin></Button>
               </children>
            </FlowPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
